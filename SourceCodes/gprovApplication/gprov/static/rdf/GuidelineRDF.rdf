    dct:partOf gprov:ADA_Guidelines ;
    sio:hasValue "Metformin should be continued when used in combination with other agents, including insulin, if not contraindicated and if tolerated. " ;
    prov:influencer gprov:ADA_Recommendation_11_Grade ;
